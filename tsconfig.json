/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "strictPropertyInitialization": false,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "sourceMap": true,
    "paths": {
      "ngx-alert-message": [
        "dist/ngx-alert-message"
      ],
      "ngx-array": [
        "dist/ngx-array"
      ],
      "ngx-check-permission": [
        "dist/ngx-check-permission"
      ],
      "ngx-color-converter": [
        "dist/ngx-color-converter"
      ],
      "ngx-color-palette": [
        "dist/ngx-color-palette"
      ],
      "ngx-cursor-hover": [
        "dist/ngx-cursor-hover"
      ],
      "ngx-default-image": [
        "dist/ngx-default-image"
      ],
      "ngx-device-info": [
        "dist/ngx-device-info"
      ],
      "ngx-logs": [
        "dist/ngx-logs"
      ],
      "ngx-random": [
        "dist/ngx-random"
      ],
      "ngx-resize-element": [
        "dist/ngx-resize-element"
      ],
      "ngx-slight-scroll": [
        "dist/ngx-slight-scroll"
      ],
      "ngx-strings": [
        "dist/ngx-strings"
      ],
      "ngx-tags-input-box": [
        "dist/ngx-tags-input-box"
      ],
      "ngx-toast-notify": [
        "dist/ngx-toast-notify"
      ]
    },
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "useDefineForClassFields": false,
    "lib": [
      "ES2022",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
